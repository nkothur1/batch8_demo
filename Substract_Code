#include<stdio.h>
int main(){
  int number1, number2, difference;
  
  //Asking for input
  printf("Enter first number: ");
  scanf("%d", &number1);
  printf("Enter second number: ");
  scanf("%d", &number2);
  
  difference = number1 - number2;
  printf("Difference of %d and %d is: %d", number1, number2, difference);
  return 0;
} 
