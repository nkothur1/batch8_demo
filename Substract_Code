#include<stdio.h>
int main(){
  int no1, no2, difference;
  
  //Asking for input
  printf("Enter first number: ");
  scanf("%d", &no1);
  printf("Enter second number: ");
  scanf("%d", &no2);
  
  difference = no1 - no2;
  printf("Difference of %d and %d is: %d", no1, no2, difference);
  return 0;
}
